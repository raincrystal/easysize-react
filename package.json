{
  "name": "widget-plugin",
  "version": "1.0.0",
  "description": "eCommerce Product React Component Widget Plugin",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "serve": "node server.js",
    "watch": "watchify ./src/widget.js -t babelify -o ./build/demo.js -v",
    "build": "browserify ./src/widget.js -t babelify -o ./build/demo.js",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raincrystal/easysize-react.git"
  },
  "keywords": [
    "easysize"
  ],
  "author": "Dmitry Musatov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raincrystal/easysize-react/issues"
  },
  "homepage": "https://github.com/raincrystal/easysize-react#readme",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "nodemon": "^2.0.6",
    "watchify": "^3.11.1"
  }
}
